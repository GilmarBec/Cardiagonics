<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Licence,</th>
      <th>Brand,</th>
      <th>Company,</th>
      <th>Water,</th>
      <th>Fuel,</th>
      <th>Gas,</th>
      <th>Oil,</th>
      <th>Temperature,</th>
      <th>Pollution,</th>
      <th>Pressure,</th>
      <th>Speed,</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.licence, %></td>
        <td><%= car.brand, %></td>
        <td><%= car.company, %></td>
        <td><%= car.water, %></td>
        <td><%= car.fuel, %></td>
        <td><%= car.gas, %></td>
        <td><%= car.oil, %></td>
        <td><%= car.temperature, %></td>
        <td><%= car.pollution, %></td>
        <td><%= car.pressure, %></td>
        <td><%= car.speed, %></td>
        <td><%= car.user %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
